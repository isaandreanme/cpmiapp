{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "awcodes/filament-quick-create": "^3.2",
        "awcodes/filament-versions": "^2.0",
        "awcodes/light-switch": "^1.0",
        "aymanalhattami/filament-slim-scrollbar": "^2.0",
        "azishapidin/indoregion": "^3.0",
        "barryvdh/laravel-dompdf": "^2.2",
        "bezhansalleh/filament-shield": "3.1.0",
        "filament/filament": "v3.2.2",
        "guzzlehttp/guzzle": "^7.2",
        "hasnayeen/themes": "^3.0",
        "jeffgreco13/filament-breezy": "v2.2.1.1",
        "jenssegers/blade": "^2.0",
        "kenepa/multi-widget": "^2.0",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.8",
        "laraveldaily/laravel-invoices": "^3.2",
        "leandrocfe/filament-apex-charts": "3.0",
        "malzariey/filament-daterangepicker-filter": "2.3.0",
        "marjose123/filament-lockscreen": "2.0",
        "njxqlus/filament-progressbar": "^1.0",
        "psr/simple-cache": "2.0",
        "pxlrbt/filament-excel": "^2.2",
        "swisnl/filament-backgrounds": "^1.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "files":[
            "app/Helper/Helper.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
